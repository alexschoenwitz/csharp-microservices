{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Blueprint": {
    "ServiceName": "BlueprintService",
    "RetryAttempts": 3,
    "RequestTimeout": "00:00:30",
    "MaxConcurrentRequests": 100
  },
  "Database": {
    "ConnectionString": "Host=postgres;Port=5432;Database=blueprints;Username=postgres;Password=postgres;",
    "CommandTimeout": 30,
    "MaxPoolSize": 100,
    "ConnectionIdleTimeout": "00:05:00"
  },
  "Telemetry": {
    "OtlpEndpoint": "http://otel-collector:4317"
  }
